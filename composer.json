{
  "name": "orchid/settings",
  "description": "Settings for The Laravel Framework.",
  "keywords": [
    "laravel",
    "settings",
    "database",
    "json",
    "key",
    "value"
  ],
  "require": {
    "php": ">=7.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "tabuna",
      "email": "bliz48rus@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchid\\Setting\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Orchid\\Setting\\Providers\\SettingServiceProvider"
      ],
      "aliases": {
        "Setting": "Orchid\\Setting\\Facades\\Setting"
      }
    }
  },
  "minimum-stability": "alpha",
  "prefer-stable": true
}
